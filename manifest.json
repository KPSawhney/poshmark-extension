{
  "manifest_version": 3,
  "name": "Poshmark Helper",
  "version": "1.0",
  "description": "A simple Chrome extension to help with Poshmark navigation.",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "cookies",
    "identity"
  ],
  "oauth2": {
    "client_id": "YOUR_GOOGLE_API_CLIENT_ID",
    "scopes": ["openid"]
  },
  "background": {
    "service_worker": "background.js"
  }
}
